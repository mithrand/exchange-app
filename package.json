{
  "name": "exchange-app",
  "version": "1.0.0",
  "description": "exchange-app",
  "main": "index.ts",
  "repository": "https://github.com/mithrand/exchange-app.git",
  "author": "Antonio Torres <antonio.torres@williamhill.com>",
  "license": "MIT",
  "private": true,
  "engine": {
    "node": ">=14.17.6",
    "yarn": ">=1.22.10"
  },
  "scripts": {
    "build": "yarn clean:build && webpack --config webpack/webpack.prod.js",
    "clean:all": "yarn clean:build && yarn clean:modules",
    "clean:build": "rm -rf dist",
    "clean:modules": "rm -rf dist",
    "commit": "cz",
    "git-cz": "yarn commit",
    "lint": "eslint 'src/**/*.{js,ts,tsx}' --fix",
    "start": "webpack serve --config webpack/webpack.dev.js",
    "test:unit": "jest --coverage",
    "test": "yarn lint && yarn test:unit"
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "devDependencies": {
    "@testing-library/react": "^12.1.0",
    "@types/jest": "^27.0.2",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "clean-webpack-plugin": "^4.0.0",
    "commitizen": "^4.2.4",
    "cz-emoji": "^1.3.1",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^14.0.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.26.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "fork-ts-checker-webpack-plugin": "^6.3.3",
    "html-webpack-plugin": "^5.3.2",
    "husky": "^7.0.2",
    "jest": "^27.2.1",
    "prettier": "^2.4.1",
    "ts-jest": "^27.0.5",
    "ts-loader": "^9.2.6",
    "typescript": "^4.4.3",
    "webpack": "^5.54.0",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.2.1",
    "webpack-merge": "^5.8.0",
    "webpack-stats-plugin": "^1.0.3"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
